mas masternode {

	agent explorer1 {
   		focus: common.agent_directory
   		join: explorer_wp
   	}
	agent explorer2 {
   		focus: common.agent_directory
   		join: explorer_wp
   	}
	agent explorer3 {
   		focus: common.agent_directory
   		join: explorer_wp
   	}
   
 	workspace common {
	    artifact agent_directory: directory.AgentDirectory()
	}
    
 	workspace explorer_wp {
	}

    class-path: lib, ext
        
    // agent source path
    asl-path: src/agt
              src/agt/inc

    platform: cartago("infrastructure")
    
}

